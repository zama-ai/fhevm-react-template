{
  "name": "@cloak-sdk/cli",
  "version": "0.1.0",
  "description": "CLI tools for Cloak SDK project setup and deployment",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cloak": "./dist/index.js"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "watch": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run --coverage",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@cloak-sdk/core": "workspace:*",
    "commander": "^12.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "ora": "^8.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/node": "~18.19.50",
    "typescript": "~5.8.2",
    "vitest": "~2.1.8"
  },
  "keywords": [
    "fhevm",
    "confidential",
    "blockchain",
    "cli",
    "tooling"
  ],
  "author": "Cloak SDK Team",
  "license": "MIT"
