# Cloak SDK Next.js Demo Environment Variables
# Copy this file to .env.local and fill in your values

# WalletConnect Project ID (get from https://cloud.walletconnect.com/)
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=your_project_id_here

# Cloak SDK Contract Addresses (update after deployment)
NEXT_PUBLIC_COUNTER_CONTRACT_ADDRESS=0x1234567890123456789012345678901234567890
NEXT_PUBLIC_VOTING_CONTRACT_ADDRESS=0x2345678901234567890123456789012345678901
NEXT_PUBLIC_BANK_CONTRACT_ADDRESS=0x3456789012345678901234567890123456789012

# Network Configuration
NEXT_PUBLIC_CHAIN_ID=11155111
NEXT_PUBLIC_RPC_URL=https://sepolia.infura.io/v3/your_infura_api_key_here
NEXT_PUBLIC_SEPOLIA_WEBSOCKET=wss://sepolia.infura.io/ws/v3/your_infura_api_key_here

# FHEVM Configuration
NEXT_PUBLIC_FHEVM_NETWORK=sepolia
NEXT_PUBLIC_CHAIN_ID_GATEWAY=55815
NEXT_PUBLIC_GATEWAY_URL=https://gateway.sepolia.zama.ai

# FHEVM Contract Addresses (from Zama's docs)
NEXT_PUBLIC_FHEVM_EXECUTOR_CONTRACT=0x848B0066793BcC60346Da1F49049357399B8D595
NEXT_PUBLIC_ACL_CONTRACT=0x687820221192C5B662b25367F70076A37bc79b6c
NEXT_PUBLIC_HCU_LIMIT_CONTRACT=0x594BB474275918AF9609814E68C61B1587c5F838
NEXT_PUBLIC_KMS_VERIFIER_CONTRACT=0x1364cBBf2cDF5032C47d8226a6f6FBD2AFCDacAC
NEXT_PUBLIC_INPUT_VERIFIER_CONTRACT=0xbc91f3daD1A5F19F8390c400196e58073B6a0BC4
NEXT_PUBLIC_DECRYPTION_ORACLE_CONTRACT=0xa02Cda4Ca3a71D7C46997716F4283aa851C28812
NEXT_PUBLIC_DECRYPTION_ADDRESS=0xb6E160B1ff80D67Bfe90A85eE06Ce0A2613607D1
NEXT_PUBLIC_INPUT_VERIFICATION_ADDRESS=0x7048C39f048125eDa9d678AEbaDfB22F7900a29F

# FHEVM Relayer (via local proxy to avoid CORS issues)
NEXT_PUBLIC_RELAYER_URL=http://localhost:3000/relayer

# App Configuration
NEXT_PUBLIC_APP_NAME=Cloak SDK Demo
NEXT_PUBLIC_APP_DESCRIPTION=Universal FHEVM SDK for confidential dApps

# Development
NEXT_PUBLIC_DEBUG=true
NODE_ENV=development

# Logger Configuration
NEXT_PUBLIC_LOG_LEVEL=debug
NEXT_PUBLIC_LOGGER_ENABLED=true